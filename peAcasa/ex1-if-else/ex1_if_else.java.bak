 
  
  
class ex_if_else {
    public static void main(String[] args)
    {
        char ch = 'a';
		char[] v = {'a','e','i','o'};
		char[] c = {'b','c','d','f','g','h'};
		
		for ( int i=0; i<10; i++){
			
		if ( ch == v[i]){
		System.out.println("vocala");}
		else if( ch == c[i]){
		System.out.println("consoana");}
		}
		
		
    }
}